<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Active Edge | @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/font-awesome/css")
    @RenderSection("styles", required: false)
  <link rel="shortcut icon" type="image/ico" href="~/favicon.ico">
</head>
<body>

    <!-- Wrapper-->
    <div id="wrapper">

        <!-- Navigation -->
        @Html.Partial("_Navigation")

        <!-- Page wraper -->
        <div id="page-wrapper" class="gray-bg">

            <!-- Top Navbar -->
            @Html.Partial("_TopNavbar")

            <!-- Main view  -->
            @RenderBody()

            <!-- Footer -->
            @Html.Partial("_Footer")

        </div>
        <!-- End page wrapper-->

    </div>
    <!-- End wrapper-->

    @Scripts.Render(Bundles.Scripts.Jquery)
    @Scripts.Render(Bundles.Scripts.Bootstrap)
    @Scripts.Render(Bundles.Scripts.MetsiMenu)
    @Scripts.Render(Bundles.Scripts.Pace)
    @Scripts.Render(Bundles.Scripts.SlimScroll)
    @Scripts.Render(Bundles.Scripts.Inspinia)
    @RenderSection("scripts", required: false)
</body>
</html>
